doctype html
html(lang="en")
  head
    meta(charset="UTF-8")
    meta(name="viewport", content="width=device-width, initial-scale=1.0")
    link(rel="stylesheet" href="/css/style.css")
    title Document
  body
    h1 Users list
    ul(class="list")
      each user in users
        li(class="item")
          img(src=`${images[String(user.id)]}`)
          h2(class="name") Name: #{user.name}
          p(class="mail") Mail: #{user.mail}
